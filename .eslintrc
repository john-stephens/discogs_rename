{
  "plugins": [
    "jsdoc"
  ],
  "extends": [
    "airbnb-base",
    "plugin:jsdoc/recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "overrides": [
    {
      "files": [ "*.js" ],
      "rules": {
        "no-use-before-define": 0,
        "no-console": 0
      }
    }
  ]
}
